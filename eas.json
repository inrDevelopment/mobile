{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle",
        "distribution": "store",
        "env": {
          "ANDROID_GOOGLE_SERVICES_JSON": "@ANDROID_GOOGLE_SERVICES_JSON_2"
        }
      },
      "ios": {
        "distribution": "store"
      },
      "autoIncrement": true
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "ANDROID_GOOGLE_SERVICES_JSON": "@ANDROID_GOOGLE_SERVICES_JSON_2"
        }
      },
      "ios": {}
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "alcides@inr.com.br"
      }
    }
  }
}
